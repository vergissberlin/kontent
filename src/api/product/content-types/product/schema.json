{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "uid": {
      "type": "uid",
      "required": true
    },
    "Name": {
      "type": "string",
      "required": true
    },
    "shop": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::shop.shop",
      "inversedBy": "products"
    },
    "Images": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "Description": {
      "type": "richtext",
      "maxLength": 1024,
      "minLength": 12
    },
    "Status": {
      "type": "enumeration",
      "enum": [
        "Draft",
        "Public",
        "Closed"
      ],
      "default": "Draft",
      "required": true
    }
  }
}
